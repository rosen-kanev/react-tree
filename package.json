{
  "name": "react-tree",
  "version": "0.1.0",
  "description": "An accessible tree view component",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "rollup -c",
    "serve": "vite preview",
    "test": "jest",
    "prepare": "husky install"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/react-tree.umd.js",
  "module": "./dist/react-tree.esm.js",
