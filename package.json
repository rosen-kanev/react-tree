    "react-tree",
    "react tree component",
    "react component"
  ],
  "sideEffects": false,
  "author": "Rosen Kanev <rosen.kanev@mentormate.com>",
  "homepage": "https://github.com/rosen-kanev/react-tree#readme",
  "repository": "git@github.com:rosen-kanev/react-tree.git"
}
